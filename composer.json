{
  "name": "makedo/jsonrpc",
  "version": "0.0.1",
  "type": "library",
  "description": "Json RPC server implementation for php with PSR-7 and PSR-11 support",
  "license": "MIT",
  "keywords": ["jsonrpc", "json"],
  "authors": [
    {
      "name": "makedo",
      "email": "krivousa@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Makedo\\JsonRpc\\": "src/Makedo/JsonRpc"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\Makedo\\JsonRpc\\": "test/Test/Makedo/JsonRpc"
    }
  },
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "psr/http-message": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/container": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "laminas/laminas-diactoros": "^2.6"
  },
  "config": {
    "bin-dir": "bin"
  },
  "suggest": {
    "laminas/laminas-diactoros": "PSR-7 implementation",
    "nyholm/psr7": "More lightweight PSR-7 implementation"
  }
}
